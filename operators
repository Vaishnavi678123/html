<head>
<title> java script basics</title>
</head>
<body>
<!--arithmetic operators-->
<script>
x = 10;
y = 5;
document.write(x+y, "<br>");
document.write(x-y, "<br>");
document.write(x*y, "<br>");
document.write(x/y, "<br>");
document.write(x%y, "<br>");
</script>
<!-- assignment operators -->
<script>
x = 10;
y = 5;
document.write(y+=1, "<br>");
document.write(y-=2, "<br>");
document.write(x*=2, "<br>");
document.write(x/=2, "<br>");
document.write(x%=2, "<br>");
</script>
<!--relational operators-->
<script>
x = 10;
y = 5;
</script>
<!-- assignment operators -->
<script>
x = 10;
y = 5;
document.write(y+=1, "<br>");
document.write(y-=2, "<br>");
document.write(x*=2, "<br>");
document.write(x/=2, "<br>");
document.write (x%=2,"<br>");
</script>
<!--relational operators -->
<script>
x = 10;
y = 5;
document.write(x<=y, "<br>");
document.write(x>=y, "<br>");
document.write(x==y, "<br>");
document.write(x!=y, "<br>");
document.write(x<y,"<br>");
document.write(x>y, "<br>");
</script>
<!--logical operators-->
<script>
x = 10;
y = 5;
document.write(x&&y, "<br>");
document.write(x||y, "<br>");
document.write(!x, "<br>");
</script>
</body>
</html>
